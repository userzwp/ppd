
page{
  height: 100%;
}
.cates{
 height: 100%;
}
.cates_container{
 height: calc( 100vh  - 90rpx);
 display: flex;
}
.cates_container .left_menu{
flex: 2;
}
.left_menu .active{
  color: var(--themeColor);
}
.left_menu .menu_item{
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
}

.cates_container .right_content{
flex: 5;
padding-bottom: 10rpx;
}
.goods_group .goods_tilte{
 height: 80rpx;
 display: flex;
 justify-content: center;
 align-items: center;

}
.goods_tilte .delimiter{
  color:#ccc;
  padding: 0 10rpx;
}

.goods_tilte .title{
 color: #000;
}
.goods_group .goods_list{
  display: flex;
  flex-wrap: wrap;
}
.goods_list .navigator{
width: 33.33%;
text-align: center;
height: 100%;
display: flex;
flex-direction: column;
}
.goods_list .navigator image{
  width: 70%;

}
.goods_list .navigator .goods_name{

font-size: 28rpx;
}
